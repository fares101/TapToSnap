# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

def all_pods
    # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!
    
    # *** DO POD UPDATE BY REPLACING 5 SPACES WITH 5 SPACES AND # ***
    
    pod 'SVProgressHUD'     , '= 2.2.5'
    pod 'SnapKit'     , '= 4.2.0'
    pod 'Alamofire'     , '= 4.8.0'
    pod 'SwiftMessages'     , '= 7.0.1'
    pod 'Material'     , '= 2.16.4'
    pod 'Motion'
    pod 'AFDateHelper'     , '= 4.2.7'
    pod 'DeviceKit'     , '= 2.2.0'
    pod 'DSFloatingButton'
    # *** DO POD UPDATE BY REPLACING 5 SPACES WITH 5 SPACES AND # ***
    
end
    

target 'TapToSnap' do
  all_pods
end
